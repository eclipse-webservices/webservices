     <element name="prefix" type="xsd:int"/>
     <element name="number" type="xsd:int"/>
    </sequence>
   </complexType>
   <complexType name="findAddressResponse">
     <sequence>
      <element name="findAddressReturn" nillable="true" type="impl:Address"/>
     </sequence>
   </complexType>
   <complexType name="saveAddress">
