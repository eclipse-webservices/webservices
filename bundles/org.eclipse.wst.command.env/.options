# Debugging options for the org.eclipse.wst.env plugin.

# Turn on debugging master switch for the org.eclipse.wst.env plugin.
org.eclipse.wst.env/debug=true

# Turn on different tracing level
org.eclipse.wst.env/trace/info=true
org.eclipse.wst.env/trace/warning=true
org.eclipse.wst.env/trace/error=true

# Turn on data flow tracing
org.eclipse.wst.env/trace/data=false

# Turn on command tracing
org.eclipse.wst.env/trace/command=true

# Turn on emitter tracing
org.eclipse.wst.env/trace/emitter=false
